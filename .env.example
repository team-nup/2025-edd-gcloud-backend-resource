# Google Cloud Project Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-gcp-project-id

# Required: Vertex AI Location/Region
VERTEX_AI_LOCATION=your-region

# Optional: Deployment region
REGION=your-deployment-region

# Optional: Cloud Run service name (default: edd-backend-resource)
SERVICE_NAME=your-service-name

# Optional: Artifact Registry repository name (default: edd-hackathon-repo)
REPO_NAME=your-repo-name

# Optional: Docker image tag (default: latest)
IMAGE_TAG=latest

# Optional: Cloud Run configuration
MEMORY=1Gi
CPU=1
MAX_INSTANCES=10
MIN_INSTANCES=0
TIMEOUT=300
CONCURRENCY=80